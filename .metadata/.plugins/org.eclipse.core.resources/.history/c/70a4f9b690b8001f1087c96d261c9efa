package PolimorfismoExercicio_novo2;

public abstract class Carro implements Veiculo{
	
	boolean ligado;

	@Override
	public String emissaoDeSom() {
		return "VRum vrum";
	}
	@Override
	public boolean ligarVeiculo(boolean ligado) {
		if(ligado == true) {
		System.out.println("Veiculo ligado");
		}else {
			System.out.println("Veiculo desligado ");
		}
		return ligado;
		
	}

}
